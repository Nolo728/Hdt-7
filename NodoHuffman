class NodoHuffman extends ArbolHuffman {
    public final ArbolHuffman left, right; // subtrees
 
    public NodoHuffman(ArbolHuffman l, ArbolHuffman r) {
        super(l.frecuencia + r.frecuencia);
        left = l;
        right = r;
    }
}
 
